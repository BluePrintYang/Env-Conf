# git command alias
alias gal='git add .'
alias gcm='git commit -m'
alias gco='git checkout'
alias gdf='git diff'
alias gpl='git pull'
alias gps='git push'
alias grt='cd $(git rev-parse --show-toplevel)'
alias gs='git status'
# add some scripts
export PATH="/Users/<user>/<project_path>/scripts/shell:$PATH"